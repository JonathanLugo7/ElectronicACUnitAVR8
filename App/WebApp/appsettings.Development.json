{
   "Logging": {
      "LogLevel": {
         "Default": "Information",
         "Microsoft": "Warning",
         "Microsoft.Hosting.Lifetime": "Information"
      }
   },

   "StaticFiles": {
      "Cache": {
         "Cache-Control": "no-cache, no-store",
         "Pragma": "no-cache",
         "Expires": "-1"
      }
   }
}
